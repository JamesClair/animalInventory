<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" prefHeight="200" prefWidth="320" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.MainController">
    <children>
      <VBox alignment="CENTER" layoutX="110.0" prefHeight="200.0" prefWidth="320.0" spacing="10.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Button maxWidth="73.0" mnemonicParsing="false" onAction="#onActionCreateAnimal" text="CREATE">
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
            </Button>
            <Button mnemonicParsing="false" onAction="#onActionDisplayAnimal" text="DISPLAY" />
            <Button maxWidth="73.0" mnemonicParsing="false" onAction="#onActionExit" text="EXIT" />
         </children>
      </VBox>
    </children>
</AnchorPane>
